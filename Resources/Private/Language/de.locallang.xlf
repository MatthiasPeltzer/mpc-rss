        <body>
            <!-- General -->
            <trans-unit id="rss.title">
                <source>RSS Feed</source>
                <target>RSS-Feed</target>
            </trans-unit>
            <trans-unit id="rss.no_feeds">
                <source>No RSS feeds configured. Please add feeds in the content element.</source>
                <target>Keine RSS-Feeds konfiguriert. Bitte fügen Sie Feeds im Inhaltselement hinzu.</target>
            </trans-unit>
            <trans-unit id="rss.no_items">
                <source>No items found in the selected feeds.</source>
                <target>Keine Einträge in den ausgewählten Feeds gefunden.</target>
            </trans-unit>
            <trans-unit id="rss.loading">
                <source>Loading RSS feeds...</source>
                <target>RSS-Feeds werden geladen...</target>
            </trans-unit>
            <trans-unit id="rss.error">
                <source>Error loading RSS feeds. Please try again later.</source>
                <target>Fehler beim Laden der RSS-Feeds. Bitte versuchen Sie es später erneut.</target>
            </trans-unit>

