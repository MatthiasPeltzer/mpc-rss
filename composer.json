{
  "name": "mpc/mpc-rss",
  "description": "TYPO3 extension to fetch and render RSS feeds grouped by category, date or author.",
  "type": "typo3-cms-extension",
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Matthias Peltzer",
      "email": "mail@mpeltzer.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mpc\\MpcRss\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "mpc_rss"
    }
  },
  "require": {
    "php": ">=8.2",
    "typo3/cms-core": "^13.4 || ^14.0",
    "typo3/cms-extbase": "^13.4 || ^14.0",
    "typo3/cms-fluid": "^13.4 || ^14.0"
